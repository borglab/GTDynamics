<robot name="cart_pole">
    <link name="l0">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <box size="0.05 0.05 0.05"/>
            </geometry>
        </visual>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="20" />
            <inertia ixx="0.0083333" ixy="0" ixz="0" iyy="0.0083333" iyz="0" izz="0.0083333"/>
        </inertial>
    </link>
    <link name="l1">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <box size="0.05 0.05 0.05"/>
            </geometry>
        </visual>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="20" />
            <inertia ixx="0.0083333" ixy="0" ixz="0" iyy="0.0083333" iyz="0" izz="0.0083333"/>
        </inertial>
    </link>
    <link name="l2">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.15" />
            <geometry>
                <cylinder length="0.3" radius="0.01"/>
            </geometry>
        </visual>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 .15" />
            <mass value="0.3" />
            <inertia ixx=".00225" ixy="0" ixz="0" iyy=".00225" iyz="0" izz=".000015"/>
        </inertial>
    </link>

    <joint name="j0" type="prismatic">
        <parent link="l0"/>
        <child link="l1"/>
        <axis xyz="0 1 0"/>
        <limit effort="999999.0" lower="-999999" upper="999999" velocity="999999"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
    </joint>

    <joint name="j1" type="continuous">
        <parent link="l1"/>
        <child link="l2"/>
        <axis xyz="1 0 0"/>
        <origin rpy="3.1415 0 0" xyz="0 0 0"/>
    </joint>
</robot>